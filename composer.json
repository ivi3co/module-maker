{
    "name": "ivi3/module-maker",
    "description": "A description for module-maker.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Milad Parirokh",
            "email": "ivi3.p@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "illuminate/support": "^v8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Ivi3\\ModuleMaker\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivi3\\ModuleMaker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ivi3\\ModuleMaker\\ModuleMakerServiceProvider"
            ],
            "aliases": {
                "ModuleMaker": "ivi3\\ModuleMaker\\"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.0"
    }
}
